    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-text-recognition": "^0.1.1",
    "expo-updates": "^29.0.12",
    "expo-web-browser": "~15.0.10",
    "lucide-react-native": "^0.544.0",
    "openai": "^6.17.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
