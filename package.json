{
  "name": "dnanir",
  "version": "1.1.8",
  "description": "دنانير - Smart Iraqi-themed financial app for expense and income tracking",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:setup": "./ios-setup.sh",
    "ios:prebuild": "npx expo prebuild --platform ios",
    "ios:pod-install": "cd ios && pod install && cd ..",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "eject": "expo eject",
    "copy:android-icons": "./copy-android-icons.sh",
    "android:prebuild": "npx expo prebuild --platform android && npm run copy:android-icons",
    "build:android:aab": "cd android && ./gradlew bundleRelease --no-daemon -PreactNativeArchitectures=arm64-v8a",
    "build:android:apk": "cd android && ./gradlew assembleRelease --no-daemon -PreactNativeArchitectures=arm64-v8a"
  },
  "dependencies": {
    "@expo-google-fonts/cairo": "^0.4.2",
    "@expo/vector-icons": "^15.0.2",
